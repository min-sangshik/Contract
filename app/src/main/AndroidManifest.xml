<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.dtimes.contract">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_SMS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/contract_logo2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Req1WriteActivity" />
        <activity android:name=".Req2SendActivity" />
        <activity android:name=".Agree1CheckActivity" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".Agree2WriteActivity" />
        <activity android:name=".Agree3AgreedActivity" />
        <activity android:name=".ContractListViewActivity"></activity>
    </application>

</manifest>